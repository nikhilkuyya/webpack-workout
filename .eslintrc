{
    "extends": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "env": {
        "es6": true
    },
    "rules": {
        "no-var": "error",
        "no-console": "warn",
        "prefer-const": "error"
    }
}